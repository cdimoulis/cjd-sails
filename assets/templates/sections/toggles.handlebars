<h5>
  Toggles
</h5>
<div class='mdl-grid'>
  <div class='mdl-cell mdl-cell--12-col-phone mdl-cell--6-col'>
    {{view 'components/toggle/checkbox' data=components.checkbox_one}}
  </div>
  <div class='mdl-cell mdl-cell--12-col-phone mdl-cell--6-col'>
    {{view 'components/toggle/checkbox' data=components.checkbox_two}}
  </div>
</div>
<div class='mdl-grid'>
  <div class='mdl-cell mdl-cell--12-col-phone mdl-cell--6-col'>
    {{view 'components/toggle/radio' data=components.radio_one}}
  </div>
  <div class='mdl-cell mdl-cell--12-col-phone mdl-cell--6-col'>
    {{view 'components/toggle/radio' data=components.radio_two}}
  </div>
</div>
<div class='mdl-grid'>
  <div class='mdl-cell mdl-cell--6-col'>
    {{view 'components/toggle/icon' data=components.icon_one}}
  </div>
  <div class='mdl-cell mdl-cell--6-col'>
    {{view 'components/toggle/icon' data=components.icon_two}}
  </div>
</div>
<div class='mdl-grid'>
  <div class='mdl-cell mdl-cell--6-col'>
    {{view 'components/toggle/switch' data=components.switch_one}}
  </div>
  <div class='mdl-cell mdl-cell--6-col'>
    {{view 'components/toggle/switch' data=components.switch_two}}
  </div>
</div>
